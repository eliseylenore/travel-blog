@{
    ViewData["Title"] = "Travel Blog";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@model IEnumerable<TravelBlog.Models.Experience>
<h2>Experiences!</h2>

@foreach (var experience in Model)
{
    <h4>@Html.DisplayFor(modelExperience => experience.Name) | @Html.ActionLink("Details", "Details", new { id = experience.ExperienceId })</h4>
}



<p>@Html.ActionLink("Add new experience", "Create")</p>